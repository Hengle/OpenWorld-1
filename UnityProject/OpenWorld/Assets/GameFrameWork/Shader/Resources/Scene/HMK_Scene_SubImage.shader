			half4 frag ( VertexOutput IN  ) : SV_Target
			{
				UNITY_SETUP_INSTANCE_ID( IN );
				UNITY_SETUP_STEREO_EYE_INDEX_POST_VERTEX( IN );

				#if defined(ASE_NEEDS_FRAG_WORLD_POSITION)
				float3 WorldPosition = IN.worldPos;
				#endif
				float4 ShadowCoords = float4( 0, 0, 0, 0 );

				#if defined(ASE_NEEDS_FRAG_SHADOWCOORDS)
					#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				half2 texCoord356 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				// *** BEGIN Flipbook UV Animation vars ***
				// Total tiles of Flipbook Texture
				float fbtotaltiles352 = trunc( _Rows ) * trunc( _Columns );
				// Offsets for cols and rows of Flipbook Texture
				float fbcolsoffset352 = 1.0f / trunc( _Rows );
				float fbrowsoffset352 = 1.0f / trunc( _Columns );
				// Speed of animation
				float fbspeed352 = _TimeParameters.x * _Speed;
				// UV Tiling (col and row offset)
				float2 fbtiling352 = float2(fbcolsoffset352, fbrowsoffset352);
				// UV Offset - calculate current tile linear index, and convert it to (X * coloffset, Y * rowoffset)
				// Calculate current tile linear index
				float fbcurrenttileindex352 = round( fmod( fbspeed352 + 0.0, fbtotaltiles352) );
				fbcurrenttileindex352 += ( fbcurrenttileindex352 < 0) ? fbtotaltiles352 : 0;
				// Obtain Offset X coordinate from current tile linear index
				float fblinearindextox352 = round ( fmod ( fbcurrenttileindex352, trunc( _Rows ) ) );
				// Multiply Offset X by coloffset
				float fboffsetx352 = fblinearindextox352 * fbcolsoffset352;
				// Obtain Offset Y coordinate from current tile linear index
				float fblinearindextoy352 = round( fmod( ( fbcurrenttileindex352 - fblinearindextox352 ) / trunc( _Rows ), trunc( _Columns ) ) );
				// Reverse Y to get tiles from Top to Bottom
				fblinearindextoy352 = (int)(trunc( _Columns )-1) - fblinearindextoy352;
				// Multiply Offset Y by rowoffset
				float fboffsety352 = fblinearindextoy352 * fbrowsoffset352;
				// UV Offset
				float2 fboffset352 = float2(fboffsetx352, fboffsety352);
				// Flipbook UV
				half2 fbuv352 = texCoord356 * fbtiling352 + fboffset352;
				// *** END Flipbook UV Animation vars ***
				half4 tex2DNode353 = tex2D( _SubImage, fbuv352 );
				
				float4 screenPos = IN.ase_texcoord4;
				half4 ase_screenPosNorm = screenPos / screenPos.w;
				ase_screenPosNorm.z = ( UNITY_NEAR_CLIP_VALUE >= 0 ) ? ase_screenPosNorm.z : ase_screenPosNorm.z * 0.5 + 0.5;
				float screenDepth362 = LinearEyeDepth(SHADERGRAPH_SAMPLE_SCENE_DEPTH( ase_screenPosNorm.xy ),_ZBufferParams);
				half distanceDepth362 = abs( ( screenDepth362 - LinearEyeDepth( ase_screenPosNorm.z,_ZBufferParams ) ) / ( _DepthFadedistance ) );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( tex2DNode353 * _Color ).rgb;
				float Alpha = ( tex2DNode353.a * saturate( distanceDepth362 ) );
				float AlphaClipThreshold = _AlphaClip;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
					LODDitheringTransition( IN.clipPos.xyz, unity_LODFade.x );
				#endif

				#ifdef ASE_FOG
					Color = MixFog( Color, IN.fogFactor );
				#endif

				return half4( Color, Alpha );
			}

			ENDHLSL
		}

		
